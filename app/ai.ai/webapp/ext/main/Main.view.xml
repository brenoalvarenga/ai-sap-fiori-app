<mvc:View xmlns:core="sap.ui.core"
          xmlns:mvc="sap.ui.core.mvc"
          xmlns="sap.m"
          xmlns:macros="sap.fe.macros"
          xmlns:html="http://www.w3.org/1999/xhtml"
          controllerName="ai.ai.ext.main.Main">

  <Page id="Main" title="{i18n>MainTitle}">
    <content>
      <VBox id="ButtonVBox" class="sapUiSmallMargin">
        <u:FileUploader
                        id="fileUploader"
                        name="myFileUpload"
                        width="300px"
                        change="onChange"/>
        <Button id="sendPrompt" text="Send prompt" press="onPress" />
      </VBox>
    </content>
  </Page>
</mvc:View>
